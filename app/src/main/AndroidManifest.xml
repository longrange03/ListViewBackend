<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androstock.populatelistview" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="g620apps.listviewbackend.IsoAdapter" />
        <activity android:name="g620apps.listviewbackend.XMLParser" />
        <activity android:name="g620apps.listviewbackend.ImageLoader" />
        <activity android:name="g620apps.listviewbackend.MemoryCache" />
        <activity android:name="g620apps.listviewbackend.Utils" />
        <activity android:name="g620apps.listviewbackend.FileCache" />
        <activity android:name="g620apps.listviewbackend.MainActivity"></activity>

    </application>

</manifest>





